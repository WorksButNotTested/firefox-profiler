{
  "name": "Local",
  "image": "profiler",
  "runArgs": [
    "--cap-add=SYS_PTRACE",
    "--security-opt",
    "seccomp=unconfined"
  ],
  "mounts": [
    "source=${localWorkspaceFolder},target=/home/ws,type=bind",
    "source=${localEnv:HOME},target=/home/share,type=bind"
  ],
  "forwardPorts": [4242],
  "postStartCommand": "nohup bash -c 'cd /root/profiler; yarn start&' >/dev/null 2>&1"
}
